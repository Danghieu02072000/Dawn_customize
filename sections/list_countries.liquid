<div class="list-ul">
  <h2 class="list-ul__heading">{{ section.settings.heading }}</h2>
   {% for block in section.blocks %}
     <ul>
       {%  for block in section.blocks %}
       <li>{{ block.settings.text_item }}</li>
       {% endfor %}
     </ul>
    {% endfor %}
    {% if section.settings.show_block %}
        <p>{{ section.settings.show_block }}</p>
    {% endif %}
</div>

{% schema %}
  {
    "name" : "List UL",
    "settings": [
      {
        "type": "text",
        "id" : "heading",
        "label": "Heading",
        "default": "this is Heading"
      },
      {
        "type": "number",
        "id": "show_block",
        "label" : "show Block",
        "default": 15
      },
      {
        "type": "range",
        "id": "number_column",
        "label": "Number Column",
         "min": 1,
        "max": 4,
        "step": 1,
        "default": 4
      }
    ],
    "blocks" : [
      {
        "type": "column",
        "name": "Column",
        "settings" : [ 
          {
          "type": "text",
          "id": "text_item",
          "label" : "Item",
          "default": "This is li"
        }]

      }
    ],
    "presets": [
      {
        "name": "List Ul"
      }
    ]
  }
{% endschema %}

