
<h1>{{ section.settings.heading }}</h1>
<div class="customeContainer owl-carousel">



{% schema %}
  {
    "name": "Hero",
    "settings": [
      {
        "type": "text",
        "id": "heading",
        "label": "heading",
        "default": "This is Slick Slider"
      }
    ],
    "blocks": [
      {
        "type": "column",
        "name": "Slide",
        "settings": [
          {
            "type": "text",
            "id": "title",
            "label": "Title"
          }, {
            "type": "image_picker",
            "id": "image",
            "label": "Image"
          }
        ]
      }
    ],
    "presets": [
      {
        "name": "Slick Slider"
      }
    ]
  }
{% endschema %}